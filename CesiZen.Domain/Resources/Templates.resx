  <data name="TEMPLATE_VERIFICATION_EMAIL" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html lang="en"&gt;
&lt;head&gt;
    &lt;meta charset="UTF-8"&gt;
    &lt;meta name="viewport" content="width=device-width, initial-scale=1.0"&gt;
    &lt;title&gt;Email Verification&lt;/title&gt;
&lt;/head&gt;
&lt;body style="font-family:Arial, sans-serif; line-height:1.6; color:#333; margin:0; padding:0;"&gt;
    &lt;table width="100%" cellspacing="0" cellpadding="0" style="max-width:600px; margin:0 auto; background-color:#f9f9f9;"&gt;
        &lt;tr&gt;
            &lt;td align="center" style="padding:40px 20px;"&gt;
                &lt;table width="100%" cellspacing="0" cellpadding="0"&gt;
                    &lt;tr&gt;
                        &lt;td align="center"&gt;
                            &lt;h1 style="color:#007bff;"&gt;Verify Your Email&lt;/h1&gt;
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td align="center" style="padding:20px 0;"&gt;
                            &lt;p&gt;Thank you for signing up! Please click the button below to verify your email address and activate your account.&lt;/p&gt;
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td align="center" style="padding:20px 0;"&gt;
                            &lt;table cellspacing="0" cellpadding="0"&gt;
                                &lt;tr&gt;
                                    &lt;td align="center" style="border-radius:5px;" bgcolor="#007bff"&gt;
                                        &lt;a href="{{url}}" target="_blank" style="display:inline-block; padding:12px 24px; font-family:Arial, sans-serif; font-size:16px; color:#ffffff; text-decoration:none; font-weight:bold;"&gt;Verify Email&lt;/a&gt;
                                    &lt;/td&gt;
                                &lt;/tr&gt;
                            &lt;/table&gt;
                        &lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td align="center" style="padding-top:20px;"&gt;
                            &lt;p&gt;If you didn't request this verification, please ignore this email.&lt;/p&gt;
                        &lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/table&gt;
            &lt;/td&gt;
        &lt;/tr&gt;
    &lt;/table&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
